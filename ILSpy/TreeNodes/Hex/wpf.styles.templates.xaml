    <Style x:Key="MDTableIndexTextBlockStyle"
           BasedOn="{StaticResource {x:Type TextBlock}}"
           TargetType="{x:Type TextBlock}">
        <Setter Property="TextTrimming" Value="CharacterEllipsis" />
        <Setter Property="HorizontalAlignment" Value="Right" />
        <Setter Property="VerticalAlignment" Value="Center" />
    </Style>

    <Style x:Key="MDTableTextBoxStyle"
           BasedOn="{StaticResource {x:Type TextBox}}"
           TargetType="{x:Type TextBox}">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderBrush" Value="Transparent" />
    </Style>
    <Style x:Key="MDTableTextBoxReadOnlyStyle"
           BasedOn="{StaticResource MDTableTextBoxStyle}"
           TargetType="{x:Type TextBox}">
        <Setter Property="IsReadOnly" Value="True"/>
    </Style>

    <DataTemplate DataType="{x:Type tnhex:MetaDataTable1VM}">
        <Border TextBlock.Foreground="{DynamicResource SystemColorsControlText}" Background="{DynamicResource SystemColorsControl}">
            <ListView
                VirtualizingStackPanel.IsVirtualizing="True"
                VirtualizingStackPanel.VirtualizationMode="Recycling"
                SelectionMode="Single"
                ItemsSource="{Binding Collection}"
                SelectedItem="{Binding SelectedItem}">
                <ListView.View>
                    <GridView AllowsColumnReorder="False">
                        <GridViewColumn Header="RID    ">
                            <GridViewColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock Style="{StaticResource MDTableIndexTextBlockStyle}" Text="{Binding RidString, Mode=OneTime}" />
                                </DataTemplate>
                            </GridViewColumn.CellTemplate>
                        </GridViewColumn>
                        <GridViewColumn Header="Token">
                            <GridViewColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBox Style="{StaticResource MDTableTextBoxReadOnlyStyle}" Text="{Binding TokenString, Mode=OneTime}" />
                                </DataTemplate>
                            </GridViewColumn.CellTemplate>
                        </GridViewColumn>
                        <GridViewColumn Header="Offset">
                            <GridViewColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBox Style="{StaticResource MDTableTextBoxReadOnlyStyle}" Text="{Binding OffsetString, Mode=OneTime}" />
                                </DataTemplate>
                            </GridViewColumn.CellTemplate>
                        </GridViewColumn>
                        <GridViewColumn Header="{Binding Column0Name, Mode=OneTime}" Width="100">
                            <GridViewColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBox Style="{StaticResource MDTableTextBoxStyle}" Text="{Binding Column0.DataFieldVM.StringValue, ValidatesOnDataErrors=True, ValidatesOnExceptions=True, UpdateSourceTrigger=LostFocus}" />
                                </DataTemplate>
                            </GridViewColumn.CellTemplate>
                        </GridViewColumn>
                    </GridView>
                </ListView.View>
            </ListView>
        </Border>
    </DataTemplate>

